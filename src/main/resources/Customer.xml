<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycompany.convirgance.vs.mybatis.CustomerMapper"> 
  <select id = "getAll" resultMap = "result">
   SELECT * FROM CUSTOMER 
  </select>
  <resultMap id = "result" type = "com.mycompany.convirgance.vs.mybatis.Customer">
    <result property = "customer_id" column = "CUSTOMER_ID"/>
    <result property = "discount_code" column = "DISCOUNT_CODE"/>
    <result property = "zip" column = "ZIP"/>
    <result property = "name" column = "NAME"/>
    <result property = "addressline1" column = "ADRESSLINE1"/>
    <result property = "adressline2" column = "ADDRESSLINE2"/>
    <result property = "city" column = "CITY"/>
    <result property = "state" column = "STATE"/>
    <result property = "phone" column = "PHONE"/>
    <result property = "fax" column = "FAX"/>
    <result property = "email" column = "EMAIL"/>
    <result property = "credit_limit" column = "CREDIT_LIMIT"/>
  </resultMap>
</mapper>